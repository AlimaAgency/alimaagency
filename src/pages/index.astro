---
import FeaturesWithHeading from "../components/Bento";
import FAQ from "../components/FAQ";
import FloatingNavDemo from "../components/Float";
import Footer from "../components/Footer";
import Hero from "../components/Hero";
import NavBar from "../components/NavBar";
import OurApproach from "../components/OurApproach";
import { HeroScrollDemo } from "../components/UnleashScroll";
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Alima Agency</title>
	</head>
	<body class="bg-[#0c1e1e]">
		<main class="mx-auto max-w-6xl">
			<FloatingNavDemo client:load/>
			<NavBar client:load/>
			<Hero client:load/>
			<FeaturesWithHeading />
			<HeroScrollDemo client:load/>
			<!-- <Services /> -->
			<OurApproach/>
			<FAQ client:load/>
			<Footer />
		</main>
		<script src="../../node_modules/preline/dist/preline.js"></script>
	</body>
</html>
